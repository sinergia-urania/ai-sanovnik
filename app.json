{
  "expo": {
    "name": "DreamCodex AI",
    "slug": "ai-sanovnik",
    "owner": "mare82",
    "version": "1.7",
    "orientation": "portrait",
    "scheme": "com.mare82.aisanovnik",
    "android": {
      "package": "com.mare82.aisanovnik",
      "googleServicesFile": "google-services.json",
      "versionCode": 7,
      "permissions": [
        "com.google.android.gms.permission.AD_ID",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": false,
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "com.mare82.aisanovnik"
            }
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": false,
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "com.mare82.aisanovnik",
              "host": "auth",
              "pathPrefix": "/callback"
            },
            {
              "scheme": "com.mare82.aisanovnik",
              "host": "auth"
            },
            {
              "scheme": "com.mare82.aisanovnik",
              "host": "auth",
              "pathPattern": ".*"
            }
          ]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.mare82.aisanovnik",
      "buildNumber": "7",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "Used to show relevant ads and support the app."
      }
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-audio",
        {
          "microphonePermission": "Allow DreamCodex AI to access your microphone."
        }
      ],
      "expo-tracking-transparency",
      "expo-web-browser",
      "expo-notifications",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2786609619751533~9204499845",
          "iosAppId": "ca-app-pub-2786609619751533~1584002451"
        }
      ]     
    ],
    "notification": {
      "iosDisplayInForeground": true,
      "color": "#9b87f5"
    },
    "extra": {
      "eas": {
        "projectId": "e5abc1e7-6bb5-4c80-bb82-42c30f17d129"
      }
    }
  }
}
